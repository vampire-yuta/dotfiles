{
	"languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform","tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
	}
}

